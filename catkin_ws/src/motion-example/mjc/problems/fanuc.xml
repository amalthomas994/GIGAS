<mujoco model="fanuc">
    <compiler angle="radian" meshdir="fanuc-melodic-devel/stls/"/>
    <size njmax="500" nconmax="100"/>
    <default/>
    <asset>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_base_link" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_base_link.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_base_link" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_base_link.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_1" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_1.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_1" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_1.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_visual_link_2" file="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_visual_link_2.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_collision_link_2" file="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_collision_link_2.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_3" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_3.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_3" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_3.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_visual_link_4" file="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_visual_link_4.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_collision_link_4" file="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_collision_link_4.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_5" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_5.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_5" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_5.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_6" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_6.stl"/>
        <mesh name="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_6" file="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_6.stl"/>
        <mesh name="table" file="123.stl" scale="0.1 0.1 0.1"/>
        <mesh name="plant" file="plant.stl" scale="0.1 0.1 0.1"/>
    </asset>
    <option>
        <flag sensornoise="enable" />
    </option>
    <worldbody>
        <geom type="plane" size="5 5 0.1"/>
        <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.239216 0.239216 0.239216 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_base_link"/>
        <geom type="mesh" rgba="0.239216 0.239216 0.239216 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_base_link"/>
        


        <body name="link_1" pos="0 0 0.33">
            <joint name="joint_1" pos="0 0 0" axis="0 0 1" limited="true" range="-2.9671 2.9671" damping="0.1"/>
            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_1"/>
            <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_1"/>
            <body name="link_2" pos="0.075 0 0">
                <joint name="joint_2" pos="0 0 0" axis="0 1 0" limited="true" range="-1.5708 2.4435" damping="0.1"/>
                <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_visual_link_2"/>
                <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_collision_link_2"/>
                <body name="link_3" pos="0 0 0.4">
                    <joint name="joint_3" pos="0 0 0" axis="0 -1 0" limited="true" range="-2.5831 4.0143" damping="0.1"/>
                    <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_3"/>
                    <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_3"/>
                    <body name="link_4" pos="0 0 0.075">
                        <joint name="joint_4" pos="0 0 0" axis="-1 0 0" limited="true" range="-3.3161 3.3161" damping="0.1"/>
                        <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_visual_link_4"/>
                        <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic5l_collision_link_4"/>
                        <body name="link_5" pos="0.41 0 0">
                            <joint name="joint_5" pos="0 0 0" axis="0 -1 0" limited="true" range="-2.0944 2.0944" damping="0.1"/>
                            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_5"/>
                            <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_5"/>
                            <body name="link_6" pos="0.08 0 0">
                                <joint name="joint_6" pos="0 0 0" axis="-1 0 0" limited="true" range="-6.2832 6.2832" damping="0.1"/>
                                <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.15 0.15 0.15 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_visual_link_6"/>
                                <geom type="mesh" rgba="0.15 0.15 0.15 1" mesh="fanuc_lrmate200ic_support_meshes_lrmate200ic_collision_link_6"/>
                                <camera mode="fixed" name="eye_in_hand" pos="0.05 0 0"  quat="0 0.707108 0.707108 0" fovy="75"/>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>

        <body name="target" pos="1 .1 .01">
			<joint armature="0" axis="1 0 0" damping="0" limited="true" name="target_x" pos="0 0 0" range="-.27 .27" ref=".1" stiffness="0" type="slide"/>
			<joint armature="0" axis="0 1 0" damping="0" limited="true" name="target_y" pos="0 0 0" range="-.27 .27" ref="-.1" stiffness="0" type="slide"/>
			<geom conaffinity="0" contype="0" name="target" pos="0 0 0" rgba="0.9 0.2 0.2 1" size="0.05" type="sphere"/>
		</body>
    </worldbody>
    <actuator>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint_1" name="torq_j1"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint_2" name="torq_j2"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint_3" name="torq_j3"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint_4" name="torq_j4"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint_5" name="torq_j5"/>
        <motor ctrllimited="true" ctrlrange="-12.0 12.0" joint="joint_6" name="torq_j6"/>

    </actuator>
    <sensor>
        <jointpos joint="joint_2" noise="0.2"/>
        <jointvel joint="joint_2" noise="0.2"/>
    </sensor>
</mujoco>
