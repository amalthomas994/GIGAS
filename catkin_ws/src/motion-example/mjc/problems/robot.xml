<mujoco model="fanuc_lrmate200ic5l">
    <compiler inertiafromgeom="true"/>
	
	<option gravity="0 0 -9.81" integrator="RK4" timestep="0.01"/>
    <actuator>
        <!--  Physical limits of the actuator.  -->
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint1" name="torq_j1"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint2" name="torq_j2"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint3" name="torq_j3"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint4" name="torq_j4"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint5" name="torq_j5"/>
        <motor ctrllimited="true" ctrlrange="-80.0 80.0" joint="joint6" name="torq_j6"/>
    </actuator>
    <asset>
        <mesh name="link0" file="meshes/link0.stl" />
        <mesh name="link1" file="meshes/link1.stl" />
        <mesh name="link2" file="meshes/link2.stl" />
        <mesh name="link3" file="meshes/link3.stl" />
        <mesh name="link4" file="meshes/link4.stl" />
        <mesh name="link5" file="meshes/link5.stl" />
        <mesh name="link6" file="meshes/link6.stl" />
        <mesh name="link0_vis" file="meshes/link0_vis.stl" />
        <mesh name="link1_vis" file="meshes/link1_vis.stl" />
        <mesh name="link2_vis" file="meshes/link2_vis.stl" />
        <mesh name="link3_vis" file="meshes/link3_vis.stl" />
        <mesh name="link4_vis" file="meshes/link4_vis.stl" />
        <mesh name="link5_vis" file="meshes/link5_vis.stl" />
        <mesh name="link6_vis" file="meshes/link6_vis.stl" />
        <mesh name="tomato_plant" file="meshes/tomatovine.stl"/>
        <mesh name="fanuc_cart" file="meshes/cart.stl"/>
        <mesh name="grip" file="meshes/gripper.stl"/>


        <texture type="skybox" builtin="gradient" rgb1="1 0.5 1" rgb2=".6 .8 1" width="256" height="256"/>
    </asset>
    <worldbody>
        
        <body name="floor" pos="0 0 -0.1">
            <geom type="box" size="4 3 0.1" rgba="0 0 0.5 1"/>
            <!-- 0.58-0.12 -->
            <body pos="-0.46 0 0.1">
                <geom type="mesh" contype="1" conaffinity="1" group="1" rgba="0.5 0.76 0.13 1" mesh="fanuc_cart" name="cart"/>			 
            </body>
        </body>
        
        <!-- <body pos="1 0 0.5">
            <geom type="mesh" contype="1" conaffinity="1" group="1" rgba="0.5 0.76 0.13 1" mesh="grip" name="gripper"/>			 
        </body> -->
        <body name="base" pos="0 0 0.4350">
            <!-- robot view -->
            <camera mode="fixed" name="robotview" pos="1.0 0 0.4" quat="0.653 0.271 0.271 0.653"/>
            <inertial diaginertia="0 0 0" mass="0" pos="0 0 0"/>
            <!-- mount attached here -->
            <body name="link0" pos="0 0 0">
                <inertial pos="0 0 0.05" mass="4" diaginertia="0.4 0.4 0.4" />
                <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.239216 0.239216 0.239216 1" mesh="link0_vis" name="link0_visual"/>
                <geom type="mesh" rgba="0.239216 0.239216 0.239216 1" group="0" mesh="link0" name="link0_collision"/>
                <body name="link1" pos="0 0 0.33">
                    <inertial pos="0 0 -0.07" mass="3" diaginertia="0.3 0.3 0.3" />
                    <joint name="joint1" pos="0 0 0" axis="0 0 1" limited="false" range="-2.9671 2.9671" damping="0.1"/>
                    <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="link1_vis" name="link1_visual"/>
                    <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="link1" name="link1_collision"/>
                    <body name="link2" pos="0.075 0 0">
                        <inertial pos="0 -0.1 0" mass="3" diaginertia="0.3 0.3 0.3" />
                        <joint name="joint2" pos="0 0 0" axis="0 1 0" limited="false" range="-1.5708 2.4435" damping="0.1"/>
                        <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="link2_vis" name="link2_visual"/>
                        <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="link2" name="link2_collision"/>
                        <body name="link3" pos="0 0 0.4">
                            <inertial pos="0.04 0 -0.05" mass="2" diaginertia="0.2 0.2 0.2" />
                            <joint name="joint3" pos="0 0 0" axis="0 -1 0" limited="false" range="-2.5831 4.0143" damping="0.10"/>
                            <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="link3_vis" name="link3_visual"/>
                            <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="link3" name="link3_collision"/>
                            <body name="link4" pos="0 0 0.075">
                                <inertial pos="-0.04 0.05 0" mass="2" diaginertia="0.2 0.2 0.2" />
                                <joint name="joint4" pos="0 0 0" axis="-1 0 0" limited="false" range="-3.3161 3.3161" damping="0.1"/>
                                <geom type="mesh" contype="0" conaffinity="0" group="1" rgba="0.96 0.76 0.13 1" mesh="link4_vis" name="link4_visual"/>
                                <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="link4" name="link4_collision"/>
                                <body name="link5" pos="0.41 0 0">
                                    <inertial pos="0 0 -0.15" mass="2" diaginertia="0.2 0.2 0.2" />
                                    <joint name="joint5" pos="0 0 0" axis="0 -1 0" limited="false" range="-2.0944 2.0944" damping="0.1"/>
                                    <geom type="mesh" contype="1" conaffinity="1" group="1" rgba="0.96 0.76 0.13 1" mesh="link5_vis" name="link5_visual"/>
                                    <geom type="mesh" rgba="0.96 0.76 0.13 1" mesh="link5" name="link5_collision"/>
                                    <body name="link6" pos="0.08 0 0">
                                        <inertial pos="0.06 0 0" mass="1.5" diaginertia="0.1 0.1 0.1" />
                                        <joint name="joint6" pos="0 0 0" axis="-1 0 0" limited="false" range="-6.2832 6.2832" damping="0.1"/>
                                        <geom type="mesh" contype="1" conaffinity="1" group="1" rgba="0.15 0.15 0.15 1" mesh="link6_vis" name="link6_visual"/>
                                        <geom type="mesh" rgba="0.15 0.15 0.15 1" mesh="link6" name="link6_collision"/>
                                        <body name="gripper" pos="0 0 0" quat="1 0 0 0">
                                            <inertial pos="0.06 0 0" mass="1.5" diaginertia="0.1 0.1 0.1" />
                                                <geom type="mesh" contype="1" conaffinity="1" group="1" rgba="0.69 0.702 0.728 1" mesh="grip" name="gripper"/>			 

                                            <!-- <geom pos="0 0 0" size="0.03 0.008 0.008" type="box" conaffinity="1" contype="1" group="1" name="end_effector_visual"/>
                                            <geom pos="0 0 0" size="0.03 0.008 0.008" type="box" name="end_effector_collision"/> -->
                                            <body name="right_hand" pos="0 0 0" quat="1 1 1 1">
                                                <inertial pos="0 0 0" mass="0.5" diaginertia="0.05 0.05 0.05" />
                                                <!-- This camera points out from the eef. -->
                                                <camera mode="fixed" name="eye_in_hand" pos="0.05 0 0" quat="0 0.707108 0.707108 0" fovy="75"/>
                                                <!-- to add gripper -->
                                            </body>
                                        </body>

                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>    
        </body>
        
        <!-- <geom name="obstacle" pos="0.6 0.22 0.75" rgba="0.1 0.9 0.9 1" size="0.02" type="sphere" contype="1" conaffinity="1"/>
        <geom name="obstacle2" pos="0.5 0.22 0.75" rgba="0.1 0.9 0.9 1" size="0.02" type="sphere" contype="1" conaffinity="1"/> -->
        
        <body euler="180 90 -90">
              <geom type="mesh" contype="1" conaffinity="1" group="1" rgba="0.5 0.76 0.13 1" mesh="tomato_plant" name="to"/>			 
          </body>
        
        
        
        <!-- <include file="obs.xml" /> -->

        
        
       
        
    </worldbody>
    
</mujoco>
